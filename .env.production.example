# Production Environment Variables
# Copy this file to .env.production.local and update with actual values

# Google Analytics Configuration
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Environment
NODE_ENV=production

# Application Configuration
NEXT_PUBLIC_APP_NAME=URL Extractor
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_URL=https://your-domain.com

# Performance Configuration
NEXT_PUBLIC_MAX_CONTENT_LENGTH=50000
NEXT_PUBLIC_MAX_URL_COUNT=100

# Security Configuration
NEXT_PUBLIC_ENABLE_CSP=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Vercel Configuration (automatically set by Vercel)
# VERCEL_URL=
# VERCEL_ENV=production
# VERCEL_GIT_COMMIT_SHA=